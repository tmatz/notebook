{
  "name": "notebook",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:open": "vite --open",
    "format": "tsc --noEmit && prettier --config prettier.format.js -w src package.json tsconfig.json vite.config.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "postbuild": "ncp htaccess dist/.htaccess",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@reduxjs/toolkit": "^1.7.1",
    "framer-motion": "^5.6.0",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux-observable": "^2.0.0",
    "rehype": "^12.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-react": "^7.0.4",
    "rehype-sanitize": "^5.0.1",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.1",
    "unist-util-inspect": "^7.0.0",
    "usehooks-ts": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@types/ramda": "^0.27.64",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "c8": "^7.11.0",
    "ncp": "^2.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "github:tmatz/prettier-plugin-organize-imports#optional-remove-unused-imports",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-tsconfig-paths": "^3.3.17",
    "vitest": "^0.3.2"
  }
}
